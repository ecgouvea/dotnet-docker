<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - aspnetapp</title>
    <link rel="stylesheet" href="https://d3kx8jhiwpqap1.cloudfront.net/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://d3kx8jhiwpqap1.cloudfront.net/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index">aspnetapp</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <div>
            <a href="/">Frontend Clube Extrafarma</a>
        </div>
        <div>
            <a href="/api">Backend Clube Extrafarma</a>
        </div>
        <div>
            <a href="/adm">Frontend Adm Clube Extrafarma</a>
        </div>
        <div>
            <a href="/api/adm">Backend Adm Clube Extrafarma</a>
        </div>
        <div>
            <a href="/hospital">Frontend Hospital Clube Extrafarma</a>
        </div>
        <div>
            <a href="/api/hospital">Backend Hospital Clube Extrafarma</a>
        </div>
        <div>
            <a href="/api/soap-to-rest">API SOAP-to-REST</a>
        </div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2019 - aspnetapp - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="https://d3kx8jhiwpqap1.cloudfront.net/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://d3kx8jhiwpqap1.cloudfront.net/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://d3kx8jhiwpqap1.cloudfront.net/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
